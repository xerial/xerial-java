# single-line comment

# object description (one-liner)
-object: param1: value1, param2: value2 

# object description (folded). Indentation represents nested structure
-object: 
	-param1: value1
	-param2: value2

# tab-separeted data description with a specified object schema

-address: id, name, phone 
1	leo	xxx-xxxx
2	yui	yyy-yyyy

# The above expression is equivalent to the follwoing descriptions:
-address: id:1, name:leo, phone:xxx-xxxx
-address: id:2, name:yui, phone:yyy-yyyy
 

# combination of one-line and folded attributes of an object
-object: param1:value1
	-param2:value2

# sample data (tab)
-gene: id, chr, name
1	chr1	gene1
2	chr1	gene2	
3	chr1	gene3
4	chr2	gene4

# alternative tree structures, grouped by the chr values
-chr:chr1
  -gene: id, name	
1	gene1
2	gene2
3	gene3
-chr:chr2
  -gene: id, name
4	gene4


# nested structure. relative depth of the indentation determines the nest level
- paper: id:1
 - title: "some paper"
 - reference
  - title: "reference paper"
  - author: author 1
  - author: author 2
  - author: author 3
 - year:2009  # Still in the scope of the paper (id:1)
- paper: id:2 # A new paper scope begins from here       
 - title: "another paper"
 - year:2008

